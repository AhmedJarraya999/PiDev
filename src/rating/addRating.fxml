<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.Rating?>

<AnchorPane id="AnchorPane" prefHeight="342.0" prefWidth="483.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rating.AddRatingController">
   <children>
      <BorderPane prefHeight="342.0" prefWidth="500.0">
         <top>
            <Label prefHeight="30.0" prefWidth="121.0" text="Add rating " BorderPane.alignment="TOP_LEFT">
               <BorderPane.margin>
                  <Insets left="50.0" top="30.0" />
               </BorderPane.margin>
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <textFill>
                  <LinearGradient endX="1.0" endY="1.0">
                     <stops>
                        <Stop color="#1e36cde5" />
                        <Stop color="#4059a9" offset="1.0" />
                     </stops>
                  </LinearGradient>
               </textFill>
            </Label>
         </top>
         <center>
            <Pane prefHeight="219.0" prefWidth="534.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox layoutX="31.0" layoutY="14.0" prefHeight="245.0" prefWidth="349.0" spacing="10.0">
                     <children>
                        <Rating id="rating_id" fx:id="fxrating" prefHeight="32.0" prefWidth="200.0">
                           <VBox.margin>
                              <Insets left="50.0" />
                           </VBox.margin>
                        </Rating>
                        <TextArea fx:id="fxcomment" prefHeight="92.0" prefWidth="219.0" promptText="comment">
                           <VBox.margin>
                              <Insets bottom="10.0" left="50.0" right="80.0" top="20.0" />
                           </VBox.margin>
                        </TextArea>
                        <HBox prefHeight="59.0" prefWidth="308.0">
                           <children>
                              <JFXButton fx:id="fxsub_btn" buttonType="RAISED" layoutX="118.0" layoutY="10.0" onAction="#handleButtonAction" prefHeight="33.0" prefWidth="73.0" style="-fx-border-radius: 8px; -fx-background-color: #626dc4;" text="Submit" textFill="WHITE">
                                 <font>
                                    <Font name="System Bold" size="11.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets left="40.0" />
                                 </HBox.margin>
                              </JFXButton>
                              <JFXButton fx:id="fxmod_btn" buttonType="RAISED" onAction="#handleButtonAction" prefHeight="33.0" prefWidth="75.0" style="-fx-border-radius: 8px; -fx-border-color: #626dc4;" text="Modify" textFill="#626dc4" visible="false">
                                 <font>
                                    <Font size="11.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets left="5.0" />
                                 </HBox.margin>
                              </JFXButton>
                              <JFXButton fx:id="fxdel_btn1" buttonType="RAISED" layoutX="179.0" layoutY="10.0" onAction="#handleButtonAction" prefHeight="33.0" prefWidth="75.0" style="-fx-border-radius: 8px; -fx-border-color: #626dc4;" text="Delete" textFill="#626dc4" visible="false">
                                 <font>
                                    <Font size="11.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets left="5.0" />
                                 </HBox.margin>
                              </JFXButton>
                           </children>
                           <opaqueInsets>
                              <Insets top="50.0" />
                           </opaqueInsets>
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </Pane>
         </center>
         <bottom>
            <JFXButton fx:id="btnback" buttonType="RAISED" onAction="#Back" prefHeight="33.0" prefWidth="75.0" style="-fx-border-radius: 8px; -fx-border-color: #626dc4;" text="Back" textFill="#626dc4" BorderPane.alignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
               <BorderPane.margin>
                  <Insets bottom="30.0" left="380.0" />
               </BorderPane.margin>
            </JFXButton>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
